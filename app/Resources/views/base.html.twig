<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {%include 'AcmeResourceBundle:Templates:css.html.twig'%}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>
    <body>
        {%include 'AcmeResourceBundle:Templates:js.html.twig'%}
            <!-- Header and Nav -->
        {%include 'AcmeResourceBundle:Templates:top-bar.html.twig'%}
            <!-- End Header and Nav -->
            <div class="row">
                <!-- Main Content Section -->
                <!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
                <div class="large-9 push-3 columns">
                    {% block body %}{% endblock %}
                </div>
                    <!-- Nav Sidebar -->
                    <!-- This is source ordered to be pulled to the left on larger screens -->
                    {%include 'AcmeResourceBundle:Templates:side-bar.html.twig'%}
            </div>
                <!-- Footer -->
            <footer class="row">
                <div class="large-12 columns">
                    <hr />
                 {%include 'AcmeResourceBundle:Templates:bottom-bar.html.twig'%}   
                </div>
            </footer>
    </body>
</html>
